var fs = require('fs');
var addressBookData;

module.exports = function(done) {

	fs.readFile('./adb.txt', 'utf8', function (err, data) {
		if (err) {
			throw err;
		}
		addressBookData = data.split("\n");
		for(var i = 0; i < addressBookData.length; i++){
			addressBookData[i] =addressBookData[i].split(','); 
		}
		console.log('ab-data: '+addressBookData);
		done();
	});
	this.set('storage','test');
}
